sum_tickets = 0
ticket = int(input("Введите количество билетов:"))
for age in range (ticket):
    age = int(input("Введите возраст посетителя:"))
    if age <= 18:
        sum_tickets +=0
    elif 18 <= age <= 25:
        sum_tickets += 990
    elif age > 25:
        sum_tickets += 1390
    print("Стоимость ваших билетов:", sum_tickets)
    if ticket >=3:
        sum_tickets = sum_tickets * (100-10)/100
    print("Сумма за все билеты:",int(sum_tickets))
